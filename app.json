{
  "expo": {
    "name": "URPScannerQR",
    "slug": "URPScannerQR",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/LogoApp1.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/LogoApp1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/LogoApp1.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.urpiriodev.PrototipoAppCamara"
    },
    "web": {
      "favicon": "./assets/LogoApp1.png"
    },
    "extra": {
      "eas": {
        "projectId": "3157b8b8-2811-4291-a5c0-2d76adc11c11"
      }
    }
  }
}
